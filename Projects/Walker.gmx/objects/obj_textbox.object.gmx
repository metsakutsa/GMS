        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Initialization

width = window_get_width();
height = window_get_height()/3;
padding = 8;
xpos = 0;
ypos = window_get_height()-height;

//Keep track of starting line
start = ds_list_create();
ds_list_add( start, 0 );

//Keep track of the last space and current position
count = 0;
last_space = 0;
line = 0;

//Set the message
message = "Tere, mina olen üks lahe message. Täitke kõik minu käsku, või inimesed hakkavad viga saama...";
str = ""; //stream visible on screen

</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="7" enumb="3">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Destroy the ds list to avoid memory leak
ds_list_destroy(start);

</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
